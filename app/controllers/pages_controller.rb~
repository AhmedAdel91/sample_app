class PagesController < ApplicationController
  def home
    @title = "Home"
home_path => '/'
home_url => 'http://localhost:3000/'
  end

  def contact
    @title = "Contact"
contact_path => '/contact'
contact_url => 'http://localhost:3000/contact'
  end
  
  def about
    @title = "About"
about_path => '/about'
about_url => 'http://localhost:3000/about'
  end

  def help
    @title = "Help"
help_path => '/help'
help_url => 'http://localhost:3000/help'
  end
end
